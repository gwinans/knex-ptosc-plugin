{
  "name": "knex-ptosc-plugin",
  "version": "0.0.12",
  "type": "module",
  "description": "Knex plugin to run pt-online-schema-change migrations with dry-run support",
  "keywords": [
    "knex",
    "pt-online-schema-change",
    "migration",
    "plugin"
  ],
  "author": "Geoff Winans <geoff.winans@brainfailures.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    }
  },
  "types": "./index.d.ts",
  "files": [
    "index.js",
    "index.cjs",
    "index.d.ts",
    "src/"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "vitest": "^2.0.0"
  }
}
